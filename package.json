{
  "name": "ch-web-next",
  "private": true,
  "scripts": {
    "prestart": "yarn",
    "start": "next dev -H 127.0.0.1 -p 3000",
    "build": "next build",
    "serve": "next start -H 127.0.0.1 -p 3000",
    "lint": "next lint",
    "ci:unit": "jest --passWithNoTests --ci",
    "unit": "jest --passWithNoTests --watchAll",
    "coverage": "jest --coverage",
    "ci:e2e": "start-server-and-test start http://localhost:3000 cy:run",
    "e2e": "start-server-and-test start http://localhost:3000 cy:open",
    "cy:open": "cypress open --config testFiles=**/*.e2e.test.{js,ts,tsx,jsx},integrationFolder=src/tests/e2e",
    "cy:run": "cypress run --config testFiles=**/*.e2e.test.{js,ts,tsx,jsx},integrationFolder=src/tests/e2e"
  },
  "dependencies": {
    "@emotion/css": "^11.5.0",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-regular-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "@reduxjs/toolkit": "^1.6.2",
    "@svgr/webpack": "^6.2.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^27.0.3",
    "@types/react-dom": "^17.0.11",
    "axios": "^0.22.0",
    "babel-plugin-macros": "^3.1.0",
    "classnames": "^2.3.1",
    "cookie-cutter": "^0.2.0",
    "cookies-next": "^2.0.4",
    "cypress": "^8.5.0",
    "draft-js": "^0.11.7",
    "generate-password": "^1.7.0",
    "localized-strings": "^0.2.4",
    "lodash": "^4.17.21",
    "markdown-to-jsx": "^7.1.3",
    "msw": "^0.38.1",
    "next": "12.0.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-markdown": "^7.1.0",
    "react-redux": "^7.2.5",
    "react-router-dom": "^5.3.0",
    "react-scripts": "^5.0.0",
    "react-toastify": "^8.1.0",
    "redux": "^4.1.1",
    "sass": "^1.43.3",
    "slate": "^0.71.0",
    "slate-history": "^0.66.0",
    "slate-react": "^0.71.0",
    "start-server-and-test": "^1.14.0",
    "swr": "^1.3.0",
    "wait-on": "^6.0.0",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@cypress/react": "^5.10.1",
    "@cypress/webpack-dev-server": "^1.6.0",
    "@types/draft-js": "^0.11.9",
    "@types/node": "17.0.5",
    "@types/react": "17.0.38",
    "autoprefixer": "^10.4.2",
    "eslint": "8.5.0",
    "eslint-config-next": "12.0.7",
    "eslint-plugin-cypress": "^2.12.1",
    "next-sitemap": "^2.1.12",
    "postcss": "^8.4.13",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.0.11",
    "typescript": "4.5.4"
  },
  "peerDependencies": {
    "postcss": "^8.4.13"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "cypress": {
    "component": {
      "testFiles": "**/*.e2e.test.{js,ts,tsx,jsx}",
      "componentFolder": "src/tests/e2e"
    }
  }
